{
  "name": "test-project-vite",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "Paolo Caleffi <p.caleffi@dreamonkey.com>",
  "private": true,
  "scripts": {
    "sync:testing": "yarn --cwd ../packages/testing build:local && yarn add -D ../packages/testing",
    "sync:vitest": "yarn --cwd ../packages/unit-vitest build:local && yarn add -D ../packages/unit-vitest",
    "sync:cypress": "yarn --cwd ../packages/e2e-cypress build:local && yarn add -D ../packages/e2e-cypress",
    "sync:all": "yarn sync:testing && yarn sync:vitest && yarn sync:cypress",
    "invoke:testing": "quasar ext invoke @quasar/testing",
    "invoke:vitest": "quasar ext invoke @quasar/testing-unit-vitest",
    "invoke:cypress": "quasar ext invoke @quasar/testing-e2e-cypress",
    "invoke:all": "yarn invoke:testing && yarn invoke:vitest && yarn invoke:cypress",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.2",
    "@quasar/quasar-app-extension-testing": "../packages/testing",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "../packages/e2e-cypress",
    "@quasar/quasar-app-extension-testing-unit-vitest": "../packages/unit-vitest",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitest/ui": "^0.15.0",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "cypress": "^10.10.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vitest": "^0.15.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
